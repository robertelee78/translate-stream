# API Keys (Required)
SONIOX_API_KEY=your_soniox_api_key_here
OPENAI_API_KEY=your_openai_api_key_here  # Only needed for GUI translation

# Language Configuration
PRIMARY_LANGUAGE=en           # Your primary language code
PRIMARY_LANGUAGE_NAME=English # Display name for primary language
FOREIGN_LANGUAGE=hr           # Your foreign language code  
FOREIGN_LANGUAGE_NAME=Croatian # Display name for foreign language

# Default Provider Settings
DEFAULT_TRANSCRIBE_PROVIDER=soniox
DEFAULT_TRANSLATE_PROVIDER=openai
DEFAULT_TRANSLATE_MODEL=gpt-4

# Audio Settings
AUDIO_FORMAT=pcm16
SAMPLE_RATE=16000
CHANNELS=1

# Streaming Settings
BUFFER_SIZE=4096
STREAM_TIMEOUT=30
